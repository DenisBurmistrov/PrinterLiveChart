<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" prefHeight="1920" prefWidth="1080" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <SplitPane dividerPositions="0.2" prefHeight="759" prefWidth="201">
        <AnchorPane>
            <Button fx:id="buttonQAr2" layoutX="22.0" layoutY="405.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Расход Аргона л/мин">
            <font>
               <Font name="Calibri" size="14.0" />
            </font></Button>
            <Button fx:id="buttonTempTable" layoutX="22.0" layoutY="45.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Температура стола">
            <font>
               <Font name="Calibri" size="14.0" />
            </font></Button>
            <Button fx:id="choseLogButton" layoutX="13.0" layoutY="624.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="176.0" text="Выберете Log" />

         <Button fx:id="buttonTempDosat" layoutX="22.0" layoutY="85.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Температура дозатора">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonAirFlowBuild" layoutX="22.0" layoutY="125.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Обдув построения">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonAirFlowOptics" layoutX="22.0" layoutY="165.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Обдув оптики">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonPressureInFilter" layoutX="22.0" layoutY="205.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Давление в фильтрах">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonPressureInCamera" layoutX="22.0" layoutY="245.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Давление в камере">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonTempOfCamera" layoutX="22.0" layoutY="285.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Температура камеры">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonOxygen1" layoutX="22.0" layoutY="325.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Кислород 1">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonOxygen2" layoutX="22.0" layoutY="365.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Кислород 2">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
         <Button fx:id="buttonCreateExcel" layoutX="22.0" layoutY="445.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="157.0" text="Создать Excel">
            <font>
               <Font name="Calibri" size="14.0" />
            </font>
         </Button>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="1920" prefWidth="850">
            <LineChart fx:id="liveChart" layoutX="14.0" layoutY="14.0" prefHeight="1895" prefWidth="800.0">
                <xAxis>
                    <NumberAxis side="BOTTOM" fx:id="xAxis" />
                </xAxis>
                <yAxis>
                    <NumberAxis fx:id="yAxis" side="LEFT" />
                </yAxis>
            </LineChart>
        </AnchorPane>
    </SplitPane>
</VBox>
